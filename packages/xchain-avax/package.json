    "@xchainjs/xchain-util": "^0.11.0",
    "axios": "^0.25.0",
    "ethers": "^5.6.6"
  },
  "peerDependencies": {
    "@xchainjs/xchain-client": "^0.13.2",
    "@xchainjs/xchain-crypto": "^0.2.6",
    "@xchainjs/xchain-evm": "^0.1.0-alpha",
    "@xchainjs/xchain-util": "^0.11.0",
    "axios": "^0.25.0",
    "ethers": "^5.6.6"
  }
}